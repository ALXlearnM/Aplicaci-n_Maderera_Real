@model Maderera_Aplicacion_Web.Models.Pret10Envio

<div class="container">
    <div class="row mt-5">
        <div class="col-sm-6">
            <h2>Detalles de Envío</h2>
        </div>
        <div class="col-sm-6 text-right">
            <a class="btn btn-danger" asp-action="ListadoEnv">Ver Listado</a>
        </div>
    </div>

    <hr class="text-decoration-line-through" />

    <div class="row mt-4">
        <div class="col-md-4">
            <dl class="row">
                <div class="col-sm-12">
                    <dt>
                        Numero Envio:
                    </dt>
                    <dd>
                        @if (!string.IsNullOrEmpty(Model.NroEnvio))
                        {
                            <input type="text" value="@Model.NroEnvio" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                        Numero:
                    </dt>
                    <dd>
                        @if (!string.IsNullOrEmpty(Model.TxtNro))
                        {
                            <input type="text" value="@Model.TxtNro" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                        Serie:
                    </dt>
                    <dd>
                        @if (!string.IsNullOrEmpty(Model.TxtSerie))
                        {
                            <input type="text" value="@Model.TxtSerie" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                        Envio Cantidad:
                    </dt>
                    <dd>
                        @if (Model.EnvioCant != null)
                        {
                            <input type="text" value="@Model.EnvioCant" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                        Fecha:
                    </dt>
                    <dd>
                        @if (Model.FechaEnvio != null)
                        {
                            <input type="text" value="@Model.FechaEnvio.ToString("yyyy-MM-dd")" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
            </dl>
         </div>

        <div class="col-md-4">
            <dl class="row">


               
                <div class="col-sm-12">
                    <dt>
                        Estado:
                    </dt>
                    <dd>
                        @if (!string.IsNullOrEmpty(Model.TxtEstado))
                        {
                            <input type="text" value="@Model.TxtEstado" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                        Usuario:
                    </dt>
                    <dd>
                        @if (!string.IsNullOrEmpty(Model.TxtUsuario))
                        {
                            <input type="text" value="@Model.TxtUsuario" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                        Usuario Modificador:
                    </dt>
                    <dd>
                        @if (!string.IsNullOrEmpty(Model.TxtUsuarioModificador))
                        {
                            <input type="text" value="@Model.TxtUsuarioModificador" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                        Fecha Modificacion:
                    </dt>
                    <dd>
                        @if (Model.FechaModificacion != null)
                        {
                            <input type="text" value="@Model.FechaModificacion" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>

                </dl>

        </div>

        <div class="col-md-4">
            <dl class="row">


                <div class="col-sm-12">
                    <dt>
                        Extraccion:
                    </dt>
                    <dd>
                        @if (Model.IdExtraccionNavigation != null && Model.IdExtraccionNavigation.IdExtraccion != null)
                        {
                            <input type="text" value="@Model.IdExtraccionNavigation.IdExtraccion" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                        Locacion:
                    </dt>
                    <dd>
                        @if (Model.IdLocationNavigation != null && Model.IdLocationNavigation.IdLocation != null)
                        {
                            <input type="text" value="@Model.IdLocationNavigation.IdLocation" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                        Locacion Llegada:
                    </dt>
                    <dd>
                        @if (Model.IdLocationToNavigation != null && Model.IdLocationToNavigation.IdLocation != null)
                        {
                            <input type="text" value="@Model.IdLocationToNavigation.IdLocation" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                        Usuario Modificador:
                    </dt>
                    <dd>
                        @if (Model.IdUsuarioModificadorNavigation != null && Model.IdUsuarioModificadorNavigation.IdUsuario != null)
                        {
                            <input type="text" value="@Model.IdUsuarioModificadorNavigation.IdUsuario" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                <div class="col-sm-12">
                    <dt>
                       Usuario:
                    </dt>
                    <dd>
                        @if (Model.IdUsuarioNavigation != null && Model.IdUsuarioNavigation.IdUsuario != null)
                        {
                            <input type="text" value="@Model.IdUsuarioNavigation.IdUsuario" readonly />
                        }
                        else
                        {
                            <input class="form-control" placeholder="No tiene" disabled>
                        }
                    </dd>
                </div>
                
            </dl>
        
            </div>
    </div>
</div>

